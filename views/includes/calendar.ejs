<div>
    <div style="display: block;">
        <select name="cMonth" id="">

        </select>
        <select name="cYear" id="">

        </select>
    </div>
    <br>
    <table class="noBorderTable">
        <tr>
            <% week = ["Sun","Mon", "Tues", "Wed", "Thurs", "Fri", "Sat"] %>
            <% let weekLength = week.length; i = 1; %>
            <% while(i < weekLength +1){ %>
            <th style="width: 100px;text-align: center;"><%= week[i-1] %></th> %>
            <% i++;} %>
        </tr>
        <% for(row=0;row<days.length;row++) {%>
        <tr>
            <% for(col=0;col<days[row].length;col++) {%>
            <% if(days[row][col]){ %>
            <td style="text-align:center" class=<%= todayDate === days[row][col] ? "dateColor" : "" %>>
                <%= days[row][col] %></td>
            <% }else{ %>
            <td></td>
            <% } %>
            <% } %>
        </tr>
        <% } %>
    </table>
</div>